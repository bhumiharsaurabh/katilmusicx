{
  "name": "SkyMusic",
  "logo": "https://telegra.ph/file/13fba0e9d76c406ae9ce2.jpg",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/Skyy112/musicsky",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "A name for your MusicPlayer Bot",
      "required": true,
      "value": ""
    },
    "BOT_USERNAME": {
      "description": "fill with your bot username from @BotFather but without using '@' symbol",
      "required": true
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "OWNER_USERNAME": {
      "description": "fill with your telegram account username without @",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
    "HEROKU_API_KEY": {
        "description": "Your Heroku account's API key",
        "value": "",
        "required": true
    },
    "HEROKU_APP_NAME": {
        "description": "Your Heroku account's API name",
        "value": "",
        "required": true
    },
    "UPSTREAM_REPO": {
        "description": "If you dont know this, Leave as it is",
        "value": "https://github.com/bhumiharsaurabh/katilmusicx",
        "required": true
    },
    "UPSTREAM_BRANCH": {
        "description": "Upstream Repo's Branch Name",
        "value": "main",
        "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "200"
    }
  }
}
